#!/bin/sh
# @generated by Idris 0.5.1-a4b99bd81, Chez backend
set -e # exit on any error
if [ "$(uname)" = Darwin ]; then
  DIR=$(zsh -c 'printf %s "$0:A:h"' "$0")
else
  DIR=$(dirname "$(readlink -f -- "$0")")
fi
export LD_LIBRARY_PATH="$DIR/lazy-idris2_app:$LD_LIBRARY_PATH"
export DYLD_LIBRARY_PATH="$DIR/lazy-idris2_app:$DYLD_LIBRARY_PATH"
export IDRIS2_INC_SRC="$DIR/lazy-idris2_app"
"$DIR/lazy-idris2_app/lazy-idris2.so" "$@"